<div class="row col s12 asset-not-found-page">
	<div class="asset-message center"> 
		<h3>Asset not found!</h3>
		<h4>Would you like to add this asset?</h4>
	</div>

	<ul class="collection">
		<li class="collection-item avatar">
	      	<span class="title">{{points}}</span>
	      	<p>Points Remaining</p>
	    </li>

	    <li class="collection-item avatar">
	      	<span class="title">{{title}}</span>
	      	<p>Prdouct Name</p>
	    </li>

	    <li class="collection-item avatar">
	      	<span class="title">{{ean}}</span>
	      	<p>Prdouct ID</p>
	    </li>

	   	<li class="collection-item avatar">
	      	<span class="title">{{brand}}</span>
	      	<p>Brand</p>
	    </li>

	    <li class="collection-item avatar">
	      	<span class="title">{{description}}</span>
	      	<p>Description</p>
	    </li>
  	</ul>

  	<div class="button-container">  		
    	<button class="btn orange" id="cancel">Cancel</button>
		<button class="btn bg-indigo" id="addAsset">Add Asset</button>
    </div>
</div>